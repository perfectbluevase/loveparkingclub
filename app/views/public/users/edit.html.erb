<h2>プロフィール編集</h2>
<%= form_with model: @user, url: public_user_path do |f| %>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <label for="inputName">Name</label>
  <%= f.text_field :user_name, autofocus: true, id:"inputName", placeholder:"ニックネーム"%>
  <label for="inputAteType">Age Type</label>
  <%= f.text_field :age_type, autofocus: true, id:"inputAgeType", placeholder:"年齢"%>
  <label for="inputLivingPrefecture">Living Prefecture</label>
  <%= f.text_field :living_prefecture, autofocus: true, id:"inputlivingPrefecture", placeholder:"住まい"%>
  <label for="inputName">Biobraphy</label>
  <%= f.text_field :biography, autofocus: true, id:"biography", placeholder:"自己紹介"%>
  
  <%= f.submit "変更を保存" %>
<% end %>
