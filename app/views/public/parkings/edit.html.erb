<h2>駐車場情報編集</h2>
<%= form_with model: @parking, url: public_parking_path do |f| %>
  <label for="inputImage">ParkingImage</label>
  <%= f.file_field :image, placeholder: "プロフィール画像", accept: "image/*" %>
  <label for="inputParkingName">駐車場名</label>
  <%= f.text_field :parking_name, autofocus: true, id:"inputParkingName", placeholder:"駐車場名"%>
  <label for="inputAddress">住所</label>
  <%= f.text_field :parking_address, autofocus: true, id:"inputAddress", placeholder:"駐車場住所"%>
  <label for="inputOperator">運営会社</label>
  <%= f.text_field :parking_operator, autofocus: true, id:"inputOperator", placeholder:"運営会社情報"%>
  <label for="inputFee">詳細</label>
  <%= f.text_field :parking_fee_info, autofocus: true, id:"inputFee", placeholder:"詳細"%>
  <label for="inputReview">駐車場についてのコメント</label>
  <%= f.text_field :parking_review, autofocus: true, id:"inputReview", placeholder:"コメント"%>
  <label for="input_tag">タグ</label>
  <%= f.text_area :input_tag %>

  <%= f.submit "変更を保存" %>
<% end %>
